<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="944dp"
        android:layout_height="1056dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.323"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.436"
        app:srcCompat="@drawable/green_gradient" />

    <EditText
        android:id="@+id/last_name"
        android:layout_width="193dp"
        android:layout_height="45dp"
        android:layout_marginStart="180dp"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="124dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/firstName"
        android:layout_width="193dp"
        android:layout_height="45dp"
        android:layout_marginStart="176dp"
        android:layout_marginLeft="176dp"
        android:layout_marginTop="320dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <EditText
        android:id="@+id/email_text"
        android:layout_width="193dp"
        android:layout_height="45dp"
        android:layout_marginTop="344dp"
        android:layout_marginEnd="400dp"
        android:layout_marginRight="400dp"
        android:ems="10"
        android:inputType="text"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <EditText
        android:id="@+id/password_text"
        android:layout_width="193dp"
        android:layout_height="45dp"
        android:layout_marginEnd="396dp"
        android:layout_marginRight="396dp"
        android:layout_marginBottom="476dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3" />

    <Button
        android:id="@+id/button2"
        android:layout_width="260dp"
        android:layout_height="61dp"
        android:layout_marginEnd="440dp"
        android:layout_marginRight="440dp"
        android:layout_marginBottom="276dp"
        android:text="Register"
        app:backgroundTint="#4CAF50"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3" />

    <TextView
        android:id="@+id/lastNametext"
        android:layout_width="130dp"
        android:layout_height="32dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="48dp"
        android:layout_marginRight="48dp"
        android:text="LAST NAME:"
        android:textAllCaps="true"
        android:textColor="#090808"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/last_name"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/firstNameText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="332dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:text="First Name:"
        android:textAllCaps="true"
        android:textColor="#0E0E0E"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/firstName"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/emailText"
        android:layout_width="77dp"
        android:layout_height="25dp"
        android:layout_marginStart="180dp"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="20dp"
        android:text="E-MAIL:"
        android:textAllCaps="true"
        android:textColor="#121111"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@id/driver_textView" />

    <TextView
        android:id="@+id/passwordText"
        android:layout_width="112dp"
        android:layout_height="37dp"
        android:layout_marginTop="556dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:text="Password:"
        android:textAllCaps="true"
        android:textColor="#171616"
        android:textColorHighlight="#131313"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/password_text"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="419dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toTopOf="@+id/last_name"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/electric_car"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-13dp" />

    <TextView
        android:id="@+id/cnp_textView"
        android:layout_width="65dp"
        android:layout_height="27dp"
        android:layout_marginStart="176dp"
        android:layout_marginLeft="176dp"
        android:layout_marginTop="364dp"
        android:text="CNP:"
        android:textAllCaps="true"
        android:textColor="#040404"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <EditText
        android:id="@+id/cnp_text"
        android:layout_width="193dp"
        android:layout_height="45dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/address_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="408dp"
        android:layout_marginEnd="672dp"
        android:layout_marginRight="672dp"
        android:text="Address:"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <EditText
        android:id="@+id/address_text"
        android:layout_width="193dp"
        android:layout_height="45dp"
        android:layout_marginStart="80dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="404dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintStart_toEndOf="@+id/address_textView"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/driver_textView"
        android:layout_width="126dp"
        android:layout_height="42dp"
        android:layout_marginStart="180dp"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="448dp"
        android:text="Expiration Date Driver License:"
        android:textAllCaps="true"
        android:textColor="#070707"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <EditText
        android:id="@+id/expirationtime_text"
        android:layout_width="193dp"
        android:layout_height="45dp"
        android:layout_marginTop="448dp"
        android:layout_marginEnd="396dp"
        android:layout_marginRight="396dp"
        android:ems="10"
        android:inputType="date|time"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/usernameText"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="609dp"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:text="USERNAME:"
        android:textColor="#171616"
        android:textColorHighlight="#131313"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/username_text"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <EditText
        android:id="@+id/username_text"
        android:layout_width="193dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="592dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="@+id/password_text"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/haveAcc"
        android:layout_width="236dp"
        android:layout_height="18dp"
        android:layout_marginTop="808dp"
        android:layout_marginEnd="452dp"
        android:layout_marginRight="452dp"
        android:text="Already have an account? Log in"
        android:textAllCaps="true"
        android:textColor="#090909"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

</androidx.constraintlayout.widget.ConstraintLayout>